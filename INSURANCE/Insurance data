SELECT *
FROM insurance
WHERE charges>
(
 SELECT AVG(charges)
 FROM insurance)
ORDER BY age

SELECT *
FROM insurance
WHERE bmi>25
AND
smoker ='yes'
ORDER BY age ASC

SELECT *
FROM insurance
WHERE charges=(

SELECT MAX(charges)
FROM insurance)

SELECT *
FROM insurance
WHERE charges=(

SELECT MIN(charges)
FROM insurance)


SELECT(MIN(charges)/ MAX(charges))*100 AS min_to_max_percentage
FROM insurance
