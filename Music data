USE Music;
CREATE TABLE Music
(id INT,
   title VARCHAR(500),
    Fulltitle VARCHAR(500),
    description VARCHAR(500),
    viewcount INT,
    Categories VARCHAR(100),
    tags VARCHAR(100),
    duration INT,
     duration_string TIME,
     live_status VARCHAR(100),
     thumbnail VARCHAR(100),
     Channel VARCHAR(100),
     Channel_URL VARCHAR(100),
     Channel_followers INT
    
);
